{
    "name": "coreproc/nova-fcm-campaigns",
    "description": "Send and create Firebase Cloud Notification campaigns to broadcast to mobile devices.",
    "keywords": [
        "laravel",
        "nova",
        "fcm",
        "firebase cloud notifications"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0||>=8.0",
        "guzzlehttp/guzzle": "^6.5||^7.0.1",
        "laravel-notification-channels/fcm": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Coreproc\\NovaFcmCampaigns\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Coreproc\\NovaFcmCampaigns\\NovaFcmCampaignsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
